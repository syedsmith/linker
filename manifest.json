{
    "name": "LinksLinker",
    "version": "2.0.0",
    "description": "Links Linker is used to organise links",
    "manifest_version": 3,
    "author": "syed.smith",
    "permissions": [
        "storage"
    ],
    "action":{
        "default_popup": "popup.html",
        "default_title": "Test App"
    },
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    }
    
}