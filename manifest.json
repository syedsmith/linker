{
    "name": "LinksLinker",
    "version": "1.3",
    "description": "Links Linker is used to organise links in a better and easy way",
    "manifest_version": 3,
    "author": "smith syed kasim",
    "permissions": [
        "storage"
    ],
    "action":{
        "default_popup": "popup.html",
        "default_title": "Links Linker"
    },
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    }
    
}